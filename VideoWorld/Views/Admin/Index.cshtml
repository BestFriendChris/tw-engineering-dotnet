@model List<VideoWorld.Models.Customer>

@{
    ViewBag.Title = "Admin";
}

<h2>Customers</h2>
<div class="customers">
@foreach(var customer in Model) {
    <ul>
        <li>
            @customer.DisplayName  (@customer.Username)
        </li>
    </ul>

    }
</div>
<h2>Actions</h2>
    <ul><li><a href="/admin/addcustomer">Add New Customer</a></li></ul>
    <ul><li><a href="/admin/addmovie">Add New Movie</a></li></ul>
